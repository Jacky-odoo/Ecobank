<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <record id="sequence_item_code" model="ir.sequence">
            <field name="name">Item Code</field>
            <field name="code">inventory.item.code</field>
            <field name="prefix">S-</field>
            <field name="padding">4</field>
        </record>

        <record id="sequence_inventory_in" model="ir.sequence">
            <field name="name">Inventory In Code</field>
            <field name="code">inventory.in.code</field>
            <field name="prefix">BTH-INV-IN-</field>
            <field name="padding">7</field>
        </record>

        <record id="sequence_inventory_adjustment" model="ir.sequence">
            <field name="name">Inventory Adjustment Code</field>
            <field name="code">inventory.adjustment.code</field>
            <field name="prefix">INV-ADJ-</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_inventory_out" model="ir.sequence">
            <field name="name">Inventory Out Code</field>
            <field name="code">inventory.out.code</field>
            <field name="prefix">BTH-INV-OUT-</field>
            <field name="padding">7</field>
        </record>

        <record id="sequence_inventory_item_in" model="ir.sequence">
            <field name="name">Item In Code</field>
            <field name="code">item.in.code</field>
            <field name="prefix">ITEM-IN-</field>
            <field name="padding">7</field>
        </record>

        <record id="sequence_inventory_item_out" model="ir.sequence">
            <field name="name">Item Out Code</field>
            <field name="code">item.out.code</field>
            <field name="prefix">ITEM-OUT-</field>
            <field name="padding">7</field>
        </record>

        <record id="sequence_inventory_supplier" model="ir.sequence">
            <field name="name">Supplier Code</field>
            <field name="code">supplier.code</field>
            <field name="prefix">SUP</field>
            <field name="padding">3</field>
        </record>

    </data>
</odoo>